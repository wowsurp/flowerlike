extend type Query {
	getAllEnterprise : [Enterprise]
	getEnterprise(idx:ID) : Enterprise
}

extend type Mutation {
	saveEnterprise(enterprise: SaveEnterpriseInput) : Enterprise
}

type Enterprise {
	idx : ID!
	name : String
	thumbnail : String
	regDtm : String
	intro : String
}

input SaveEnterpriseInput {
	idx : ID
	name : String
	thumbnail : String
	intro : String
}